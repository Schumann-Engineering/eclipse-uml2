<plugin
    name = "%pluginName"
    id = "org.eclipse.uml2.codegen.ecore"
    version = "1.1.0"
    provider-name = "%providerName"
    class = "org.eclipse.uml2.codegen.ecore.CodeGenEcorePlugin$Implementation">

  <requires>
    <import plugin="org.eclipse.core.runtime" />
    <import plugin="org.eclipse.core.resources"/>
    <import plugin="org.eclipse.emf.ecore" export="true"/>
    <import plugin="org.eclipse.emf.codegen.ecore" export="true"/>
    <import plugin="org.eclipse.jdt.core"/>
  </requires>

  <runtime>
    <library name="codegen.ecore.jar">
      <export name="*"/>
    </library>
  </runtime>

  <extension point="org.eclipse.emf.ecore.generated_package">
    <package 
       uri = "http://www.eclipse.org/uml2/1.1.0/GenModel" 
       class = "org.eclipse.uml2.codegen.ecore.genmodel.GenModelPackage"
       genModel = "model/GenModel.genmodel"/>
  </extension>

</plugin>

