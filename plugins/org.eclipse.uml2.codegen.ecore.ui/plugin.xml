<plugin
   name = "%pluginName"
   id = "org.eclipse.uml2.codegen.ecore.ui"
   version = "1.1.0"
   provider-name = "%providerName"
   class = "org.eclipse.uml2.codegen.ecore.ui.CodeGenEcoreUIPlugin$Implementation">

  <requires>
    <import plugin="org.eclipse.core.runtime"/>
    <import plugin="org.eclipse.core.resources"/>
    <import plugin="org.eclipse.jdt.core"/>
    <import plugin="org.eclipse.emf.codegen.ecore.ui"/>
    <import plugin="org.eclipse.emf.ecore.xmi" export="true"/>
    <import plugin="org.eclipse.uml2.codegen.ecore" export="true"/>
  </requires>

  <runtime>
    <library name="uml2.codegen.ecore.ui.jar">
      <export name="*"/>
    </library>
  </runtime>

  <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
    <factory 
       uri = "http://www.eclipse.org/uml2/1.1.0/GenModel" 
       class = "org.eclipse.uml2.codegen.ecore.genmodel.provider.GenModelItemProviderAdapterFactory" 
       supportedTypes = 
         "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
          org.eclipse.emf.edit.provider.IStructuredItemContentProvider
          org.eclipse.emf.edit.provider.ITreeItemContentProvider
          org.eclipse.emf.edit.provider.IItemLabelProvider
          org.eclipse.emf.edit.provider.IItemPropertySource" />
  </extension>

</plugin>
